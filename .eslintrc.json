{
  "rules": {
    "comma-dangle": ["error", "always-multiline"],
    "no-underscore-dangle": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/prop-types": 0
  },
  "extends": ["eslint:recommended", "airbnb"],
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "env": {
    "mocha": true,
    "browser": true
  }
}